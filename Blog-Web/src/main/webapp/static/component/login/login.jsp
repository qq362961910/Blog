<%@ page contentType="text/html;charset=UTF-8" language="java" isELIgnored="false" pageEncoding="UTF-8" %>
<div class="login">
    <div class="login-frame">
        <div class="login-close-btn-frame">
            <div class="login-close-btn"></div>
        </div>
        <div class="login-banner-frame">
            <div class="login-banner"></div>
        </div>
        <div class="login-short-message-frame">
            <span class="login-short-message">欢迎登录</span>
        </div>
        <div class="login-type-frame">
            <span class="login-type">手机登录</span>
        </div>
        <div class="login-type-hint-frame">
            <span class="login-type-hint">短信获取6位初始密码</span>
        </div>
        <div class="login-phone-input-frame">
            <input class="login-phone-input" name="phone" placeholder="手机号码"/>
        </div>
        <div class="login-password-outline-frame">
            <div class="login-password-frame">
                <input type="text" class="login-password" placeholder="验证码"/>
            </div>
            <div class="login-request-password-btn-frame">
                <button type="button" class="login-request-password-btn">获取验证码</button>
            </div>
        </div>
        <div class="login-btn-frame">
            <button type="button" class="login-btn">登录</button>
        </div>
        <div style="height: 5%"></div>
        <div class="login-third-party-support-frame">
            <ul>
                <li class="login-wei-xin-support">
                </li>
                <li class="login-wei-xin-support">
                </li>
                <li class="login-wei-xin-support">
                </li>
                <li class="login-wei-xin-support">
                </li>
            </ul>
        </div>
        <div style="height: 3%"></div>
        <div class="login-tip-frame">
            <span class="login-tip">登录过的用户请沿用之前的登录方式</span>
        </div>
    </div>
</div>
